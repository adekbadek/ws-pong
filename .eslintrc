{
  "extends": ["standard"],
  "rules": {
    "no-multiple-empty-lines": 1,
    "padded-blocks": 1,
    "camelcase": 0,
    "quotes": 1,
    "spaced-comment": 1,
    "semi": 1
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__CANVAS_WIDTH__": true,
    "__CANVAS_HEIGHT__": true,
    "__INIT_PADDLE_SPEED__": true
  }
}
